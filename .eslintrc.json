{
  "extends": [
    "airbnb",
    "airbnb-base"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-use-before-define": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "class-methods-use-this": 0
  },
  "parser": "babel-eslint"
}
