{
  "expo": {
    "name": "Sharemo",
    "description": "This project is really great.",
    "slug": "MemoApp",
    "privacy": "unlisted",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sharemo.pro"
    },
    "android": {
      "package": "jp.sharemo.pro",
      "permissions": [
        "CAMERA",
        "READ_CONTACTS",
        "READ_PHONE_STATE",
        "RECORD_AUDIO"
      ],
    }
  }
}
